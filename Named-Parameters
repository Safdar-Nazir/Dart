void main()
{
  Area_Required(2, 4, 2);
  print("");
  Area_Named(2, width: 3, breadth: 4);
}

// Required Parameters
void Area_Required(int length, int width, int breadth){
  print("Length is $length");
  print("Width is $width");
  print("Breadth is $breadth");
  print("Area = ${length * width * breadth}");
}

// Named  Parameters
void Area_Named(int length, { int width, int breadth})
{
  print("Length is $length");
  print("Width is $width");
  print("Breadth is $breadth");
  print("Area = ${length * width * breadth}");
}
